
page{
  height: 100%;
  width: 100%;
}

.top_view {
  display: flex;
  /* margin-bottom: 10rpx; */
}

.topLeftView {
  width: 50%;
  height: 120px;
  border-left: 1rpx solid black;
  border-right: 1rpx solid black;
}

.top_left_view_text{
  display: block;
  font-size: 28rpx;
  text-align: center;
  padding-top: 10px;
  padding-bottom: 10px;
}

#top_left_view_text_1{
  border: 1rpx solid black;
  /* padding-top: 10px;
  padding-bottom: 10px; */
}

.guaView {
  width: 50%;
  height: 120px;
}

.guaViewTitle {
  /* font-weight: bold; */
  display: flex;
  padding: 10rpx;
  align-items: center;
  justify-content: center;
  font-size: 12px;
}

.guaImage {
  width: 192rpx;
  /* height: 25rpx; */
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  /* margin-left: 20rpx; */
}

.luckImageList {
  display: flex;
  align-items: center;
}

#mid_view{
  border: 1rpx solid black;
  width: 100%;
  height: 136px;
}

.mid_view_text{
  display: block;
  font-size: 12px;
  padding-top: 3px;
  padding-bottom: 3px;
  padding-left: 20px;
}

#mid_view_text_1{
  text-align: center;
  text-decoration-line:underline;
  margin-top: 5px;
  padding-bottom: 5px;
}

#down_view{
  /* border: 1rpx solid black; */
  width: 100%;
  height: 136px;
}

.down_view_text{
  display: block;
  font-size: 12px;
  /* padding-top: 3px;
  padding-bottom: 3px; */
  line-height: 20px;
  padding-left: 5px;
}

#button_view{
  border: 1rpx solid black;
  width: 100%;
  height: 50px;
}

.view_btn{
  display: inline-block;
  border: 1rpx solid black;
  margin-left: 11%;
  font-size: 15px;
  height: 35px;
}

#button_text_1{
  display: block;
  font-size: 12px;
}

#tail_view{
  /* border: 1rpx solid black; */
  width: 100%;
  height: 36px;
  position: fixed;
  bottom: 1px;
}

.tail_item{
  display: inline;
}

#tail_coin {
  font-size: 12px;
  background-color: white;
  margin: 0px 60px;
}

#tail_link {
  text-decoration-line:underline;
  background-color: white;
  color:#00ff00;
  font-size: 12px;
  margin: 0px 10px;
}


