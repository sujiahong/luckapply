page {
  background-color: #647c7c;
}

.t_red {
  color: red;
}

.t_blue {
  color: royalblue;
}

.hide {
  display: none;
}

.calendar {
  /* width: 100%; */
  margin-left: 50rpx;
  margin-right: 50rpx;
  margin-top: 150rpx;
  /* margin: 200rpx 125rpx; *//* height: 600rpx; *//* background-color: #ffffff; */
  border-radius: 4rpx;
}

.time {
  padding: 16rpx 20rpx;
  background-color: wheat;
  display: flex;
}

.time view {
  flex: 1;
  font-size: 30rpx;
}

.time view text {
  font-size: 38rpx;
}

.weekName {
  /* border: 2rpx solid #000; */
  background-color: #878de0;
  width: 100%;
  display: flex;
  padding: 30rpx 0;
  font-size: 40rpx;
  color: rgb(255, 255, 255);
}

.weekName view {
  flex: 1;
  text-align: center;
}

.week {
  width: 100%;
}

.week view {
  width: 14.2%;
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  margin: 10rpx 0;
  text-align: center;
  font-size: 40rpx;
  color: #747474;
}

.week view text {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.calendarSign {
  margin-top: -120rpx;
  text-align: center;
}

.btnimg {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.btnSign {
  background-color: rgb(118, 221, 199);
  width: 230rpx;
}

.bcfff {
  background-color: rgb(255, 255, 255);
  padding-bottom: 130rpx;
}



